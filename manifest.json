{
	"name": "Awesome app built with infinityframework!",
	"origin": "*",
	"dbUri": "mongodb://localhost:27017",
	"port": 3000,
	"modules": [
		{
			"source": "local",
			"name": "if-blog"
		},
		{
			"source": "local",
			"name": "if-dailyMenu"
		},
		{
			"source": "local",
			"name": "if-cms"
		}
	],
	"views": [
		{
			"path": "/",
			"view": "index"
		},
		{
			"path": "/something/:postId/else",
			"view": "some-page",
			"context": ["infinityframe.blog.posts"]
		},
		{
			"path": "/path-that-errors",
			"view": "some-page",
			"context": ["infinityframe.blog.error"]
		}
	]
}
